pipeline{
 	agent any
 	
 	stages{
		stage ('Git Chekout Stage') {
			git 'https://github.com/DevQui/forent'
		}
		stage ('Compie Stage) {
			steps{
				withMaven(maven: 'maven_3_6_3){
					sh 'mvn clean compile'
				}
			}
			
		}
		stage ('Testing Stage') {
			steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn test'
                }
            }
		}
		
		stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn deploy'
                }
            }
        }
	}
}
